{
  "expo": {
    "name": "Gurukulyatra_Driver",
    "slug": "gurukul-yata-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "Gurukulyatra_Driver",
    "userInterfaceStyle": "light",

    "pushNotifications": {
      "enabled": true,
      "useNextNotificationsApi": true
    },

    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sulavgautam.DriverGurukulYatra",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": ["INTERNET"]
      },
      "googleServicesFile": "./google-services.json",
      "fcm": {
        "apiKey": "AIzaSyBMJG64yln6KCJpNPz5mBbvcG3G8OJcoas",
        "projectId": "gurukulyatra-d5297"
      },
      "package": "com.sulavgautam.DriverGurukulYatra",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDZifSZzVVOLp-P60kpAqG__CBxOtQjHrU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/TT-Octosquares-Medium.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "49ef7c01-b6c3-4514-a5a7-1fb7cad5974a"
      },
      "router": {
        "origin": false
      },
      "EXPO_PUBLIC_SERVER_URI": "https://gurukul-yatra.onrender.com/api/v1",
      "EXPO_PUBLIC_GOOGLE_CLOUD_API_KEY": "AIzaSyDZifSZzVVOLp-P60kpAqG__CBxOtQjHrU"
    },
    "owner": "sulavgautam"
  }
}
